{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:write-all",
    "fs:read-all",
    "upload:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/sts-gui-backend",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
